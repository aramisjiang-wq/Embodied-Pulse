{
  "name": "embodied-pulse-backend",
  "version": "1.0.0",
  "description": "Embodied Pulse Backend - API Server",
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,json,md}\"",
    "check:prisma": "./scripts/check-prisma-usage.sh",
    "db:migrate": "prisma migrate dev",
    "db:migrate:user": "prisma migrate dev --schema=prisma/schema.user.prisma",
    "db:migrate:admin": "prisma migrate deploy --schema=prisma/schema.admin.prisma",
    "db:push:admin": "prisma db push --schema=prisma/schema.admin.prisma",
    "db:push:user": "prisma db push --schema=prisma/schema.user.prisma",
    "seed:embodied-models": "tsx src/scripts/seed-embodied-models.ts",
    "seed:huggingface-authors": "tsx src/scripts/add-test-huggingface-authors.ts",
    "seed:huggingface-models": "tsx src/scripts/create-huggingface-test-data.ts",
    "db:generate": "prisma generate --schema=prisma/schema.user.prisma && prisma generate --schema=prisma/schema.admin.prisma",
    "db:generate:user": "prisma generate --schema=prisma/schema.user.prisma",
    "db:generate:admin": "prisma generate --schema=prisma/schema.admin.prisma",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "sync": "tsx src/scripts/sync-cron.ts all",
    "sync:embodied-ai": "tsx src/scripts/sync-cron.ts embodied-ai",
    "sync:jobs": "tsx src/scripts/manual-job-sync.ts",
    "restore:jobs": "tsx src/scripts/restore-jobs-limx.ts",
    "sync:github-jobs-2026": "tsx src/scripts/sync-github-jobs-2026.ts",
    "seed:manufacturers": "tsx src/scripts/seed-embodied-robot-manufacturers.ts",
    "seed:bilibili-keywords": "tsx src/scripts/init-bilibili-keywords.ts",
    "fetch:papers": "tsx src/scripts/fetch-papers-from-arxiv.ts",
    "import:embodied-repos": "tsx src/scripts/import-embodied-repos-from-list.ts",
    "import:huggingface-resources": "tsx src/scripts/import-huggingface-resources.ts",
    "add:repos-minimal": "tsx src/scripts/add-repos-from-list-minimal.ts",
    "backfill:repos": "tsx src/scripts/backfill-repos-from-github.ts",
    "backfill:repos:loop": "tsx src/scripts/backfill-repos-loop.ts",
    "reclassify:repos": "tsx src/scripts/reclassify-repos-from-list.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.11.0",
    "@huggingface/hub": "^2.7.1",
    "@prisma/client": "^5.9.0",
    "@types/multer": "^2.0.0",
    "@types/nodemailer": "^7.0.9",
    "@types/ws": "^8.18.1",
    "@types/xml2js": "^0.4.14",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.0",
    "cheerio": "^1.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^8.0.1",
    "prom-client": "^15.1.3",
    "redis": "^4.6.12",
    "uuid": "^13.0.0",
    "ws": "^8.19.0",
    "xml2js": "^0.6.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "loadtest": "^8.2.1",
    "prettier": "^3.2.4",
    "prisma": "^5.9.0",
    "supertest": "^6.3.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
